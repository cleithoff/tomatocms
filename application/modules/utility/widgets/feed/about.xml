<!DOCTYPE widget SYSTEM "http://schemas.tomatocms.com/dtd/widget_about.dtd">
<widget>
	<name>Feed</name>
	<title>Feed</title>
	<module>utility</module>
	<description>Show entries from RSS channel</description>
	<thumbnail></thumbnail>
	<author>TomatoCMS Core Team</author>
	<email>core@tomatocms.com</email>
	<version>2.0.0</version>
	<license>free</license>
	<params>
		<param name="url">
			<description>Feed URL</description>
		</param>
		<param name="title">
			<description>Title of feed box. If you want to display the title taken from feed URL, don't set value for this variable.</description>
		</param>
		<param name="limit">
			<description>Number of entries displayed</description>
		</param>
	</params>
	<resources></resources>
</widget>
