<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE layout SYSTEM "http://schemas.tomatocms.com/dtd/layout.dtd">
<layout>
	<container cols="12">
		<container cols="3" position="first">
			<widget module="page" name="pagetree">
				<title><![CDATA[Page Tree]]></title>
				<resources></resources>
				<params></params>
			</widget>
		</container>
		<container cols="9" position="last">
			<widget module="page" name="breadcrumb">
				<title><![CDATA[Breadcrumb]]></title>
				<resources></resources>
				<params>
					<param name="page_id" type="global" />
				</params>
			</widget>
			<defaultOutput />
			<widget module="core" name="localizer">
				<title><![CDATA[Localizer page content]]></title>
			</widget>
		</container>
	</container>
</layout>
